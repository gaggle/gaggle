{
  "name": "gaggle",
  "version": "0.0.0",
  "description": "Website for jonlauridsen.com",
  "author": "Jon Lauridsen",
  "private": true,
  "scripts": {
    "clean": "rm -rf .nuxt/ coverage/ dist/",
    "deploy": "BASE_URL=http://jonlauridsen.com npm run generate && scripts/deploy dist/ gh-pages",
    "deploy:beta": "BASE_URL=http://jonlauridsen.com/beta/ npm run generate && scripts/deploy dist/ gh-pages --beta",
    "dev": "nuxt",
    "generate": "nuxt generate && cp -R coverage dist/ || true",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest --no-cache && npm run lint",
    "test:submit-cov": "npm run test && cat ./coverage/lcov.info | codacy-coverage"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^2.6.0",
    "bulma": "^0.7.4",
    "cross-env": "^5.2.0",
    "lodash": "^4.17.11",
    "nuxt": "^2.4.3"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "ava": "^1.4.0",
    "babel-eslint": "^10.0.1",
    "browser-env": "^3.2.6",
    "canvas-prebuilt": "^2.0.0-alpha.14",
    "codacy-coverage": "^3.4.0",
    "concurrently": "^4.1.0",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": ">=22.3.0",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.3.0",
    "prettier": "^1.16.4",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^0.1.1",
    "require-extension-hooks-vue": "^2.0.0",
    "sass-loader": "^7.1.0"
  }
}
