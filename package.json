    "hexo-renderer-jade": "^0.3.0",
    "hexo-renderer-markdown-it": "^3.4.1",
    "hexo-renderer-stylus": "^0.3.1",
    "hexo-server": "^0.2.0",
    "hexo-tag-gist-it": "github:gaggle/hexo-tag-gist-it",
    "install": "^0.8.2",
    "number-to-words": "^1.2.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "mocha": "^3.1.2",
    "mocha-istanbul": "^0.3.0",
    "mocha-lcov-reporter": "^1.2.0",
    "parallel-mocha": "0.0.7",
